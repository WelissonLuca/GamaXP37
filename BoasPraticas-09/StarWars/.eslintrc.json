{
    "env": {
        "commonjs": false,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-tabs": "off",
        "no-unused-expressions": "off",
        "no-underscore-dangle": "off",
        "no-undef": "warn",
        "no-use-before-define": "off",
        "prefer-destructuring": "warn",
        "no-restricted-globals": "warn"
    }
}
